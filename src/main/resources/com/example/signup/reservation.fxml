<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="113.0" prefWidth="202.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ConTroller.ReservationController">
    <TextField fx:id="userIdField" promptText="User ID" />
    <TextField fx:id="terrainIdField" promptText="Terrain ID" />
